  "bugs": {
    "url": "https://gitlab.com/tmorin/imzer/issues"
  },
  "homepage": "https://gitlab.com/tmorin/imzer#readme",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "webExt": {
    "sourceDir": "public"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@fortawesome/fontawesome-free": "^5.4.1",
    "babel-loader": "^8.0.4",
