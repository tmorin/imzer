{
  "name": "imzer",
  "version": "1.2.2",
  "description": "imzer will help you to quickly share your location as well as to locate where the other guy is.",
  "scripts": {
    "web-ext": "web-ext run",
    "b:watch": "webpack --config webpack.prod.js --watch",
    "s:watch": "webpack-dev-server --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "release": "standard-version -a",
    "we:package": "web-ext lint && web-ext build",
    "we:publish": "web-ext sign",
    "fb:publish": "firebase --only hosting"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/tmorin/imzer.git"
  },
  "author": "Thibault Morin",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/tmorin/imzer/issues"
  },
  "homepage": "https://gitlab.com/tmorin/imzer#readme",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "webExt": {
    "sourceDir": "public"
  },
  "standard-version": {
    "scripts": {
      "postbump": "node ./scripts/postbump.js > postbump.log",
      "precommit": "git add -A"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@fortawesome/fontawesome-free": "^5.8.1",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.1",
    "clipboard": "^2.0.4",
    "copy-webpack-plugin": "^5.0.2",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.0",
    "firebase-tools": "^6.5.0",
    "html-webpack-plugin": "^3.2.0",
    "leaflet": "^1.4.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "sass-loader": "^7.1.0",
    "standard-version": "^5.0.2",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "web-ext": "^3.0.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "3.2.1",
    "webpack-merge": "^4.2.1"
  }
}
